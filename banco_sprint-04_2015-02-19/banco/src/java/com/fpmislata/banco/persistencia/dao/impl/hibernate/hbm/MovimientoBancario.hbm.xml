<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.dominio.MovimientoBancario" table="movimientoBancario" >
        <id column="idMovimientoBancario" name="idMovimientoBancario" type="integer" access="field" >
            <generator class="identity"/>
        </id>
        <property name="conceptoMovimientoBancario" column="conceptoMovimientoBancario" access="field" />
        <property name="cantidadMovimientoBancario" column="cantidadMovimientoBancario" access="field" />
        <property name="tipoMovimiento" >
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.fpmislata.banco.dominio.TipoMovimiento</param>                
                <param name="type">12</param>
            </type>
        </property>
        <many-to-one name="cuentaBancaria" cascade="all">
            <column name="idCuentaBancaria" />
        </many-to-one>
    </class>
</hibernate-mapping>

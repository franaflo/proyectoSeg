<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.dominio.EntidadBancaria" table="entidadBancaria" >
        <id column="idEntidadBancaria" name="idEntidadBancaria" type="integer" access="field" >
            <generator class="identity"/>
        </id>
        <property name="codigoEntidadBancaria" column="codigoEntidadBancaria" access="field" />
        <property name="nombreEntidadBancaria" column="nombreEntidadBancaria" access="field" />
        <property name="fechaCreacionEntidadBancaria" type="date" column="fechaCreacionEntidadBancaria" access="field" />
    
        
        <set name="sucursalesBancarias"  cascade="all" inverse="true"  >
            <key>
                <column name="idEntidadBancaria"  />
            </key>
            <one-to-many class="com.fpmislata.banco.dominio.SucursalBancaria" />
        </set>
        
    </class>
</hibernate-mapping>
